{
  "name": "shah-newaz/permissible-ng",
  "description": "Role Permission system for Laravel 8",
  "keywords": [
    "laravel",
    "admin",
    "backend",
    "ACL",
    "Role",
    "Permission",
    "Role Permission"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "rifat_rifats@live.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^8.0",
    "firebase/php-jwt": "^5.2",
    "vlucas/phpdotenv": "^5.2",
    "guzzlehttp/guzzle": "^7.0",
    "tymon/jwt-auth": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database",
      "database/migrations",
      "database/seeds"
    ],
    "psr-4": {
      "Shahnewaz\\PermissibleNg\\": "src/"
    },
    "files": [
      "src/helpers/general.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shahnewaz\\PermissibleNg\\Providers\\PermissibleServiceProvider"
      ],
      "aliases": {
        "Permissible": "Shahnewaz\\PermissibleNg\\Facades\\PermissibleFacade"
      }
    }
  }
}
