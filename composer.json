{
  "name": "shah-newaz/permissible-ng",
  "description": "Role Permission system for Laravel 11+",
  "keywords": [
    "laravel",
    "admin",
    "backend",
    "ACL",
    "Role",
    "Permission",
    "Role Permission"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "rifat_rifats@live.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11 | ^12",
    "vlucas/phpdotenv": "^v5.6",
    "tymon/jwt-auth": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database",
      "database/migrations",
      "database/seeds"
    ],
    "psr-4": {
      "Shahnewaz\\PermissibleNg\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shahnewaz\\PermissibleNg\\Providers\\PermissibleServiceProvider"
      ],
      "aliases": {
        "PermissibleAuth": "Shahnewaz\\PermissibleNg\\Facades\\PermissibleAuth"
      }
    }
  }
}
